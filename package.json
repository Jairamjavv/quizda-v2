{
    "name": "quizda",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start:server": "node server/index.js",
        "start:client": "cd client && npm run dev",
        "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
        "dev": "npm start",
        "install:all": "npm install && cd client && npm install && cd ../server && npm install"
    },
    "devDependencies": {
        "concurrently": "^8.2.0"
    }
}